<?xml version="1.0" encoding="utf-8" ?>
<Patch>

    <!-- Warning: This patch will break if original mod moves weapons into diff files.
         To fix this, please re-run script using new mod file paths. -->

  <Operation Class="PatchOperationSequence">
  <success>Always</success>
  <operations>

    <li Class="CombatExtended.PatchOperationFindMod">
        <modName>Star Wars - Factions</modName>
    </li>

    <!-- ========== SWFactions_SpeederKindImp ========== -->

    <li Class="PatchOperationAddModExtension">
    <xpath>Defs/PawnKindDef[defName="SWFactions_SpeederKindImp"]</xpath>
    <value>
        <li Class="CombatExtended.LoadoutPropertiesExtension">
            <primaryMagazineCount>
                <min>100</min>
                <max>200</max>
            </primaryMagazineCount>
        </li>
    </value>
    </li>

    <!-- ========== SWFactions_SpeederKindReb ========== -->

    <li Class="PatchOperationAddModExtension">
    <xpath>Defs/PawnKindDef[defName="SWFactions_SpeederKindReb"]</xpath>
    <value>
        <li Class="CombatExtended.LoadoutPropertiesExtension">
            <primaryMagazineCount>
                <min>100</min>
                <max>200</max>
            </primaryMagazineCount>
        </li>
    </value>
    </li>

  </operations>  <!-- end sequence -->
  </Operation>   <!-- end sequence -->

</Patch>

